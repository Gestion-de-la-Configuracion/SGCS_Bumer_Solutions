
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Sistema Bumer</title>

    <!-- App Favicon -->
    <link rel="shortcut icon" href="~/Source/images/favicon.ico">

    <!-- App CSS -->
    <link href="~/Source/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/css/core.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/css/components.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/css/pages.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/css/menu.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/css/responsive.css" rel="stylesheet" type="text/css" />
    <script src="~/Source/js/modernizr.min.js"></script>

    <!-- DataTables -->
    <link href="~/Source/plugins/datatables/jquery.dataTables.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/plugins/datatables/buttons.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/plugins/datatables/fixedHeader.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/plugins/datatables/responsive.bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Source/plugins/datatables/scroller.bootstrap.min.css" rel="stylesheet" type="text/css" />

    <!-- Notification css (Toastr) -->
    <link href="~/Source/plugins/toastr/toastr.min.css" rel="stylesheet" type="text/css" />


</head>
<body class="fixed-left">

    <div id="wrapper">

        <div class="topbar">

            <div class="topbar-left">
                <a href="index.html" class="logo"><span>Bu<span>mer</span></span><i class="zmdi zmdi-layers"></i></a>
            </div>

            <div class="navbar navbar-default" role="navigation">
                <div class="container">

                    <ul class="nav navbar-nav navbar-left">
                        <li>
                            <button class="button-menu-mobile open-left">
                                <i class="zmdi zmdi-menu"></i>
                            </button>
                        </li>
                        <li>
                            <h4 class="page-title">Sistema de Gestion de la Configuracion</h4>
                        </li>
                    </ul>

                    <ul class="nav navbar-nav navbar-right">
                        <li>
                            <!-- Notification -->
                            <div class="notification-box">
                                <ul class="list-inline m-b-0">
                                    <li>
                                        <a href="javascript:void(0);" class="right-bar-toggle">
                                            <i class="zmdi zmdi-T`H`E`M`E`L`O`C`K`.`C`O`M`-none"></i>
                                        </a>
                                        <div class="noti-dot">
                                            <span class="dot"></span>
                                            <span class="pulse"></span>
                                        </div>
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li class="hidden-xs">
                            <form role="search" class="app-search">
                                <input type="text" placeholder="Buscar..."
                                       class="form-control">
                                <a href=""><i class="fa fa-search"></i></a>
                            </form>
                        </li>
                    </ul>

                </div>
            </div>
        </div>

        <div class="left side-menu">
            <div class="sidebar-inner slimscrollleft">

                <!-- User -->
                <div class="user-box">
                    <div class="user-img">
                        <img src="~/Source/images/users/avatar-1.jpg" alt="user-img" title="Mat Helme" class="img-circle img-thumbnail img-responsive">
                        <div class="user-status offline"><i class="zmdi zmdi-dot-circle"></i></div>
                    </div>
                    <h5><a href="#">Mat Helme</a> </h5>
                    <ul class="list-inline">
                        <li>
                            <a href="#">
                                <i class="zmdi zmdi-settings"></i>
                            </a>
                        </li>

                        <li>
                            <a href="#" class="text-custom">
                                <i class="zmdi zmdi-power"></i>
                            </a>
                        </li>

                    </ul>
                </div>
                <!-- End User -->
                <!--- Sidebar -->
                <div id="sidebar-menu">
                    <ul>
                        <li class="text-muted menu-title">Navegación</li>

                        <li>
                            <a href="index.html" class="waves-effect"><i class="zmdi zmdi-view-dashboard"></i> <span> Principal </span> </a>
                        </li>

                        <li class="has_sub">
                            <a href="javascript:void(0);" class="waves-effect"><i class="zmdi zmdi-invert-colors"></i> <span> Usuario </span> <span class="menu-arrow"></span></a>
                            <ul class="list-unstyled">
                                <li><a href="#">Todos</a></li>
                                <li><a href="#">Equipos</a></li>
                                <li><a href="#">Tareas</a></li>
                            </ul>
                        </li>

                        <li>
                            <a href="#" class="waves-effect"><i class="zmdi zmdi-calendar"></i><span> Entregable </span></a>
                        </li>

                        <li>
                            <a href="#" class="waves-effect"><i class="zmdi zmdi-calendar"></i><span class="label label-purple pull-right">Nuevo</span><span> Proyectos </span></a>
                        </li>

                        <li>
                            <a href="#" class="waves-effect"><i class="zmdi zmdi-calendar"></i><span> Metodología </span></a>
                        </li>

                        <li>
                            <a href="#" class="waves-effect"><i class="zmdi zmdi-calendar"></i><span> Etapa </span></a>
                        </li>

                        <li>
                            <a href="#" class="waves-effect"><i class="zmdi zmdi-calendar"></i><span> Control de Cambio </span></a>
                        </li>

                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>

            </div>

        </div>

        <div class="content-page">
            <div class="content">
                <div class="container">
                    @RenderBody()
                </div>
            </div>

            <footer class="footer">
                2021 © Bumer Solutions.
            </footer>

        </div>
    </div>

    <script>
        var resizefunc = [];
    </script>

    <!-- jQuery  -->
    <script src="~/Source/js/jquery.min.js"></script>
    <script src="~/Source/js/bootstrap.min.js"></script>
    <script src="~/Source/js/detect.js"></script>
    <script src="~/Source/js/fastclick.js"></script>
    <script src="~/Source/js/jquery.slimscroll.js"></script>
    <script src="~/Source/js/jquery.blockUI.js"></script>
    <script src="~/Source/js/waves.js"></script>
    <script src="~/Source/js/jquery.nicescroll.js"></script>
    <script src="~/Source/js/jquery.scrollTo.min.js"></script>

    <!-- App js -->
    <script src="~/Source/js/jquery.core.js"></script>
    <script src="~/Source/js/jquery.app.js"></script>

    <!-- Datatables-->
    <script src="~/Source/plugins/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Source/plugins/datatables/dataTables.bootstrap.js"></script>
    <script src="~/Source/plugins/datatables/dataTables.buttons.min.js"></script>
    <script src="~/Source/plugins/datatables/buttons.bootstrap.min.js"></script>
    <script src="~/Source/plugins/datatables/jszip.min.js"></script>
    <script src="~/Source/plugins/datatables/pdfmake.min.js"></script>
    <script src="~/Source/plugins/datatables/vfs_fonts.js"></script>
    <script src="~/Source/plugins/datatables/buttons.html5.min.js"></script>
    <script src="~/Source/plugins/datatables/buttons.print.min.js"></script>
    <script src="~/Source/plugins/datatables/dataTables.fixedHeader.min.js"></script>
    <script src="~/Source/plugins/datatables/dataTables.keyTable.min.js"></script>
    <script src="~/Source/plugins/datatables/dataTables.responsive.min.js"></script>
    <script src="~/Source/plugins/datatables/responsive.bootstrap.min.js"></script>
    <script src="~/Source/plugins/datatables/dataTables.scroller.min.js"></script>

    <!-- Datatable init js -->
    <script src="~/Source/pages/datatables.init.js"></script>

    <script type="text/javascript">
        $(document).ready(function () {
            $('#datatable').dataTable();
            $('#datatable-keytable').DataTable({ keys: true });
            $('#datatable-responsive').DataTable();
            $('#datatable-scroller').DataTable({ ajax: "assets/plugins/datatables/json/scroller-demo.json", deferRender: true, scrollY: 380, scrollCollapse: true, scroller: true });
            var table = $('#datatable-fixed-header').DataTable({ fixedHeader: true });
        });
        TableManageButtons.init();

    </script>


</body>
</html>
